{
  "name": "rivalry-club-expo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:deploy": "npx ampx pipeline-deploy",
    "build:ios": "node scripts/increment-build.js && eas build --platform ios --profile production --non-interactive --auto-submit",
    "build:ios:local": "node scripts/increment-build.js && rm -rf ios && eas build --platform ios --profile production --local --non-interactive && eas submit --platform ios --path build-*.ipa --non-interactive && mv build-*.ipa ios_builds/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.3.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@tanstack/react-query": "^5.90.11",
    "aws-amplify": "^6.15.8",
    "axios": "^1.13.2",
    "expo": "^54.0.27",
    "expo-asset": "^12.0.11",
    "expo-constants": "^18.0.10",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-status-bar": "~3.0.9",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.18.0",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-sdk/client-cloudformation": "^3.943.0",
    "@aws-sdk/client-dynamodb": "^3.943.0",
    "@aws-sdk/lib-dynamodb": "^3.943.0",
    "@react-native-community/cli": "^20.0.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "aws-cdk-lib": "^2.232.1",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "~54.0.0",
    "constructs": "^10.4.3",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.14",
    "patch-package": "^8.0.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
